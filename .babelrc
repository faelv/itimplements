{
  "presets": [
    ["@babel/preset-env", {
      "targets": {
        "node": "current"
      },
      "debug": true,
      "useBuiltIns": "usage",
      "corejs": 3
    }]
  ],
  "plugins": [
    ["@babel/plugin-proposal-decorators", {
      "legacy": true
    }],
    ["@babel/plugin-proposal-class-properties", {
      "loose": true
    }],
    "istanbul"
  ]
}
